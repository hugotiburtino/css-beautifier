{
  "manifest_version": 2,
  "name": "CSS Beautifier",
  "version": "1.0.0",
  "author": "Hugo B. Tiburtino",
  "description": "Beautifies CSS files open on browser",
  "content_scripts": [
    {
      "matches": ["*://*/*.css", "*://*/*.css?*"],
      "css": ["assets/highlight/styles/atom-one-dark.css"],
      "js": ["assets/vkbeautify.js" ,"assets/highlight/highlight.pack.js" , "css_beautifier.js"]
    }
  ]
}
